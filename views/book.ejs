<% include head %>
</head>
<body>
	<% include navbar %>

	<div class="container">
		<div class="row">

			<div class="col-sm-12">
				<h1><u> Books of Books</u></h1>
      </div>
      
				<div class="col-sm-12">
          <% booksDummyData.forEach((book)=>{ %>
            
            <div class="col-md-3 col-sm-4">
              <a href="/book/<%= book.bookId%>">
                <h4> <%= book.name %> </h4>
              </a>
            </div>

          <% }) %>

        </div>

    </div>
    

    <form action="/book" method="POST">

      <input type="text" name="bookName" placeholder="book name">
      <input type="text" name="bookIsbn" placeholder="book isbn">

      <button> Add Book </button>
      
    </form>

	</div>
</body>
</html>